    "@types/lodash-es": "^4.17.12",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "5.0.4",
